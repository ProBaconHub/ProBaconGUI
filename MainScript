                                    end
                                    if i == 65 then
                                        game:GetService("ReplicatedStorage").remoteInterface.jobs.arrestPlayer:InvokeServer(232,v)
                                    end
                                end)
                                if v.Character:FindFirstChild("ForceField") then
                                    break
