            	                    task.wait(0.001)
            	                until not game.UserInputService:IsKeyDown(Enum.KeyCode.K)
            	            end
            	            if game.UserInputService:IsKeyDown(Enum.KeyCode.L) then
            	                repeat
            	                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map.ObjectiveHitboxes.C.CFrame
            	                    task.wait(0.001)
            	                until not game.UserInputService:IsKeyDown(Enum.KeyCode.L)
            	            end
            	        end
            	        if game.UserInputService:IsKeyDown(Enum.KeyCode.H) then
            	            repeat
            	                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = LastPos*CFrame.new(0, 1, 0)
            	                task.wait(0.001)
            	            until not game.UserInputService:IsKeyDown(Enum.KeyCode.H)
            	            PauseFrameLog = false
            	        end
            	        if game.UserInputService:IsKeyDown(Enum.KeyCode.P) then
            	            PauseFrameLog = true
            	        end
            	    end)
            	end
            end)
        end)
