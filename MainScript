        MadOp:NewToggle("Loop Kill Enemy (On Vehicle)", "You must be on a vehice with missiles", function(state)
            getgenv().MadLoopKillEnemy = state
            function isEnemyTeam(player)
                if game.Players.LocalPlayer.Team.Name ~= "Police" and game.Players.LocalPlayer.Team.Name ~= "Heroes" then
                    if player.Team.Name == "Police" or player.Team.Name == "Heroes" then
                        return true
                    end
                else
                    if player.Team.Name ~= "Police" and player.Team.Name ~= "Heroes" then
                        return true
                    end
                end
                return false
            end
            while getgenv().MadLoopKillEnemy and task.wait(.01) do
                for i, v in pairs(game.Players:GetChildren()) do
                    if v.Character and v.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and isEnemyTeam(v) and v.Character.Humanoid.Health > 0 then
                        if not v or not v.Character or not v.Character:FindFirstChild("Humanoid") or not (v.Character.Humanoid.Health > 0) or isEnemyTeam(v) == false then
                            break
                        end
                        local args = {
                            [1] = "BM",
                            [2] = Vector3.new(v.Character.HumanoidRootPart.Position.x, v.Character.HumanoidRootPart.Position.y, v.Character.HumanoidRootPart.Position.z)
                        }
                        game:GetService("ReplicatedStorage"):WaitForChild("Event"):FireServer(unpack(args))
                        task.wait()
                    end
                end
            end
        end)
        MadOp:NewButton("Kill All Enemy", "Terminated All Enemy!", function()
            if workspace.ObjectSelection:FindFirstChild("Buzzard") then
                local lastpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                local buzzardweusehere = workspace.ObjectSelection.Buzzard
                local returnplace = buzzardweusehere.PrimaryPart.CFrame*CFrame.new(0 , 5, 20)
                buzzardweusehere:SetPrimaryPartCFrame(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame*CFrame.new(0, 5, 5))
                task.wait()
                buzzardweusehere.DriveSeat:Sit(game.Players.LocalPlayer.Character.Humanoid)
                task.wait()
                function isEnemyTeam(player)
                    if game.Players.LocalPlayer.Team.Name ~= "Police" and game.Players.LocalPlayer.Team.Name ~= "Heroes" then
                        if player.Team.Name == "Police" or player.Team.Name == "Heroes" then
                            return true
                        end
                    else
                        if player.Team.Name ~= "Police" and player.Team.Name ~= "Heroes" then
                            return true
                        end
                    end
                    return false
                end
                local tasks = 0
                for i, v in pairs(game.Players:GetChildren()) do
                    if v.Character and v.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and isEnemyTeam(v) and v.Character.Humanoid.Health > 0 then
                        print("Ttarget Found: ", v.Name)
                        task.spawn(function()
                            tasks = tasks + 1
                            for _ = 1, 100 do
                                if not v or not v.Character or not v.Character:FindFirstChild("Humanoid") or not (v.Character.Humanoid.Health > 0) or isEnemyTeam(v) == false then
                                    break
                                end
                                local args = {
                                    [1] = "BM",
                                    [2] = Vector3.new(v.Character.HumanoidRootPart.Position.x, v.Character.HumanoidRootPart.Position.y, v.Character.HumanoidRootPart.Position.z)
                                }
                                game:GetService("ReplicatedStorage"):WaitForChild("Event"):FireServer(unpack(args))
                                task.wait()
                            end
                            tasks = tasks - 1
                        end)
                    end
                end
                task.wait(.25)
                repeat
                    task.wait()
                    workspace.ObjectSelection:WaitForChild(game.Players.LocalPlayer.Name.."'s Vehicle").DriveSeat:Sit(game.Players.LocalPlayer.Character.Humanoid)
                until tasks == 0 or not workspace.ObjectSelection:FindFirstChild(game.Players.LocalPlayer.Name.."'s Vehicle") or not game.Players.LocalPlayer.Character or not game.Players.LocalPlayer.Character.HumanoidRootPart
                game.Players.LocalPlayer.Character.Humanoid.Sit = false
                task.wait(.05)
                workspace.ObjectSelection:FindFirstChild(game.Players.LocalPlayer.Name.."'s Vehicle"):SetPrimaryPartCFrame(returnplace)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastpos
                task.wait(5)
                if workspace.ObjectSelection:FindFirstChild(game.Players.LocalPlayer.Name.."'s Vehicle") then
                    if workspace.ObjectSelection:FindFirstChild(game.Players.LocalPlayer.Name.."'s Vehicle").Name == game.Players.LocalPlayer.Name.."'s Vehicle" then
                        workspace.ObjectSelection:FindFirstChild(game.Players.LocalPlayer.Name.."'s Vehicle"):Destroy()
                    end
                end
            else
                game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Vehicle not found. Please wait until it respawns. We will notify you when it does"; Duration=5;})
                repeat
                    task.wait(0.01)
                until game.ObjectSelection:FindFirstChild("Buzzard")
                game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text='Vehicle respawned. You can now use "Kill All Enemy".'; Duration=5;})
            end
        end)
