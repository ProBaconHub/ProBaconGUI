local LocalPlayerTab = Window:NewTab("Local Player")
local PlayerStuffSection = LocalPlayerTab:NewSection("Local player stuff")
local PlayerStuffFlySection = LocalPlayerTab:NewSection("Fly")

PlayerStuffSection:NewSlider("Walk Speed", "Change your walk speed \r Defalt: 16", 200, 0, function(WS) -- 200 (MaxValue) | 0 (MinValue)
    getgenv().PlayerWalkSpeed = WS
end)
PlayerStuffSection:NewToggle("WalkSpeed", "Enable walkspeed", function(state)
    getgenv().PlayerWalkSpeedEnabled = state
    if state then
        task.spawn(function()
            local OldWalkSpeed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
            local changespeed = game:GetService("RunService").Heartbeat:Connect(function()
                game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = getgenv().PlayerWalkSpeed
            end)
            repeat
                task.wait(0.001)
            until not getgenv().PlayerWalkSpeedEnabled
            changespeed:Disconnect()
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = OldWalkSpeed
        end)
    end
end)

PlayerStuffSection:NewSlider("Jump Power", "Change your jump power \r Defalt: 50", 350, 0, function(JP) -- 500 (MaxValue) | 0 (MinValue)
    getgenv().PlayerJumpPower = JP
end)
PlayerStuffSection:NewToggle("JumpPower", "Enable jumppower", function(state)
    getgenv().PlayerJumpPowerEnabled = state
    if state then
        task.spawn(function()
            local OldJumpPower = game.Players.LocalPlayer.Character.Humanoid.JumpPower
            local changepower = game:GetService("RunService").Heartbeat:Connect(function()
                game.Players.LocalPlayer.Character.Humanoid.JumpPower = getgenv().PlayerJumpPower
            end)
            repeat
                task.wait(0.001)
            until not getgenv().PlayerJumpPowerEnabled
            changepower:Disconnect()
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = OldJumpPower
        end)
    end
end)
PlayerStuffSection:NewSlider("Hip Height", "Change hip height \r Defalt: 2", 100, 2, function(HH) -- 500 (MaxValue) | 0 (MinValue)
    getgenv().PlayerHipHeight = HH
end)
PlayerStuffSection:NewToggle("Hip Height", "Enable hip height", function(state)
    getgenv().PlayerHipHeightEnabled = state
    if state then
        task.spawn(function()
            local OldHipHeight = game.Players.LocalPlayer.Character.Humanoid.HipHeight
            local changepower = game:GetService("RunService").Heartbeat:Connect(function()
                game.Players.LocalPlayer.Character.Humanoid.HipHeight = getgenv().PlayerHipHeight
            end)
            repeat
                task.wait(0.001)
            until not getgenv().PlayerHipHeightEnabled
            changepower:Disconnect()
            game.Players.LocalPlayer.Character.Humanoid.HipHeight = OldHipHeight
        end)
    end
end)


