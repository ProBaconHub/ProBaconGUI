        local function SpawnVehicle(name)
            local vehicletoget = workspace.ObjectSelection[name]
            local _, boudingvector3 = vehicletoget:GetBoundingBox()
            vehicletoget:SetPrimaryPartCFrame(CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.x, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.y+(boudingvector3.y/2), game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.z))
            vehicletoget.PrimaryPart.Anchored = true
            task.wait(.05)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = vehicletoget.DriveSeat.CFrame*CFrame.new(0,2,0)
            task.wait(.25)
            vehicletoget.DriveSeat:Sit(game.Players.LocalPlayer.Character.Humanoid)
            vehicletoget.PrimaryPart.Anchored = false
        end


        local vehiclelist = {}
        for _, v in pairs(workspace.ObjectSelection:GetChildren()) do
            if v:FindFirstChild("DriveSeat") and string.find(v.Name, "'s Vehicle") == nil and table.find(vehiclelist, v.Name) == nil then
                print("Found Vehicle: ", v.Name)
                table.insert(vehiclelist, v.Name)
            end
        end
        table.sort(vehiclelist)
        local lastvehicleinput
        MadGet:NewDropdown("Get Vehicle", "Free vehicle", vehiclelist, function(currentOption)
            lastvehicleinput = currentOption
            SpawnVehicle(currentOption)
        end)
        MadGet:NewTextBox("Vehicle", "Get vehicle by input", function(txt)
            if workspace.ObjectSelection:FindFirstChild(txt) then
                if workspace.ObjectSelection[txt]:FindFirstChild("DriveSeat") and string.find(workspace.ObjectSelection[txt].Name, "'s Vehicle") == nil then
                    lastvehicleinput = txt
                    SpawnVehicle(txt)
                end
            end
        end)
        MadGet:NewButton("Spawn Again", "Spawn vehicle without inputing again", function()
            SpawnVehicle(lastvehicleinput)
        end)
