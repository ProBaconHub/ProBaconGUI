    elseif Tab == "Depthless RPG" then
        local dephlessrpgTab = Window:NewTab("Depthless RPG")
        local dephlessrpgSec = dephlessrpgTab:NewSection("", true)

        if getgenv().dephlessrpgAutoKill == nil then
            getgenv().dephlessrpgAutoKill = false
        end
        dephlessrpgSec:NewToggle("Auto kill", "", function(state)
            getgenv().dephlessrpgAutoKill = state
            if state then
                repeat
                    for _,mob in pairs(workspace.Mobs:GetChildren()) do
                        if mob:FindFirstChildOfClass("Humanoid") and game.Players.LocalPlayer.Character then
                            if mob:FindFirstChildOfClass("Humanoid").Health > 0 and game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
                                game:GetService("ReplicatedStorage").Remotes.DamageMob:FireServer(mob, nil, game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Name)
                                task.wait(0.001)
                            end
                        end
                    end
                    task.wait(0.001)
                until not getgenv().dephlessrpgAutoKill
            end
        end, getgenv().dephlessrpgAutoKill)
        dephlessrpgSec:NewButton("Mod Weapons", "", function()
            for _, v in getgc(true) do
                if type(v) == "table" then
                    if (rawget(v, "Type") and rawget(v, "Damage") and rawget(v, "Cooldown")) then
                        rawset(v, "Cooldown", 0.000)
                        rawset(v, "Autofire", true)
                        rawset(v, "Cost", nil)
                        if rawget(v, "Ranged") then
                            rawset(v.Ranged, "Velocity", 1000)
                            rawset(v.Ranged, "Acceleration", Vector3.new(0,0,0))
                            rawset(v.Ranged, "SpreadAngle", NumberRange.new(0,0))
                            rawset(v.Ranged, "Pierce", 99)
                        end
                    end
                    if rawget(v, "Potion") then
                        rawset(v, "Potion", {Effect = "Instant", Duration = 99999, MaxTime = 9999999, Rate = 1, Stat = {LuckMulti = 100, HealthMulti = 100, GoldMulti = 100, DamageMulti = 100}})
                    end
                end
            end
        end)
        getgenv().dephlessrpgautoheal = false
        local dephlessrpghook = nil
        --[[    IDK why not working
        dephlessrpghook = hookmetamethod(game, "__namecall", function(Self, ...)
            local Args = {...}
            local NamecallMethod = getnamecallmethod()
            if NamecallMethod == "FireServer" and Self.Name == "PlayerDamaged" and getgenv().dephlessrpgautoheal then
                print(Self.Name)
                return dephlessrpghook(Self, math.huge*-1)
            end
            return dephlessrpghook(Self, unpack(Args))
        end)
        ]]
        dephlessrpgSec:NewToggle("AutoHeal", "", function(state)
            getgenv().dephlessrpgautoheal = state
            if state then
                task.spawn(function()
                    repeat
                        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("PlayerDamaged"):FireServer(math.huge*-1)
                        task.wait(0.005)
                    until not getgenv().dephlessrpgautoheal
                end)
            end
        end)

        local deaphrpgx, deaphrpgy, deaphrpgz
        local function ChangeHitBoxDeaphrpg(x,y,z)
            for _, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if v:FindFirstChild("Handle") then
                    v.Handle.Size = Vector3.new(x, y, z)
                    v.Handle.Transparency = 0.9
                end
            end
            for _, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v:IsA("Tool") and v:FindFirstChild("Handle") then
                    v.Handle.CanCollide = false
                    v.Handle.Size = Vector3.new(x, y, z)
                    v.Handle.Transparency = 0.9
                end
            end
        end
        dephlessrpgSec:NewSlider("Hitbox.X", "Change the hitbox", 1000, 1, function(var)
            deaphrpgx = var
            ChangeHitBoxDeaphrpg(deaphrpgx, deaphrpgy, deaphrpgz)
        end)
        dephlessrpgSec:NewSlider("Hitbox.Y", "Change the hitbox", 1000, 1, function(var)
            deaphrpgy = var
            ChangeHitBoxDeaphrpg(deaphrpgx, deaphrpgy, deaphrpgz)
        end)
        dephlessrpgSec:NewSlider("Hitbox.Z", "Change the hitbox", 1000, 1, function(var)
            deaphrpgz = var
            ChangeHitBoxDeaphrpg(deaphrpgx, deaphrpgy, deaphrpgz)
        end)
        dephlessrpgSec:NewTextBox("Size", "Input Vector3", function(txt)
            local vector3here = stringToVector3(txt)
            print(vector3here)
            deaphrpgx = vector3here.X
            deaphrpgy = vector3here.Y
            deaphrpgz = vector3here.Z
            ChangeHitBoxDeaphrpg(vector3here.X, vector3here.Y, vector3here.Z)
        end)
    end
end
