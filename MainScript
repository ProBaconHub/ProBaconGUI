    getgenv().hitboxteamcheck = state
end, true)

local flingsection = PlayersTab:NewSection("Fling")

local probaconantifling = nil
flingsection:NewTextBox("Player", "", function(txt)
    getgenv().plrtar = txt
end)
flingsection:NewTextBox("Fling force", "", function(txt)
    getgenv().flingforce = tonumber(txt) or 500
end, "500")
flingsection:NewButton("Fling Player", "", function()
    local lastcframe = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    local tarplr = GetPlayerWithClosestName(getgenv().plrtar) or GetPlayerWithClosestName(getgenv().plrtar, true)
    pcall(function()
        for _ = 1, 100 do
            task.spawn(function()
                for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                    if v:IsA("BasePart") then
                        v.CanCollide = false
                    end
                end
            end)
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,8,0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = tarplr.Character.HumanoidRootPart.CFrame
            game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(getgenv().flingforce, getgenv().flingforce, getgenv().flingforce)
            task.wait(0.01)
        end
        for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = true
            end
        end
        for _ = 1, 50 do
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
            game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0,0,0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastcframe
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
            task.wait(0.01)
        end
    end)
end)


getgenv().probaconfling = nil
local probaconflinglastframe
flingsection:NewToggle("Loop Fling Player", "", function(state)
    local tarplr = GetPlayerWithClosestName(getgenv().plrtar) or GetPlayerWithClosestName(getgenv().plrtar, true)
    if state then
        probaconflinglastframe = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        local checkavailable = function()
            if tarplr then
                if game.Players.LocalPlayer.Character and tarplr.Character then
                    if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and tarplr.Character:FindFirstChild("Humanoid") and tarplr.Character:FindFirstChild("HumanoidRootPart") then
                        if tarplr.Character:FindFirstChild("Humanoid").Health ~= 0 then
                            return true
                        end
                    end
                end
            end
            return false
        end

        repeat
            task.wait(0.1)
        until checkavailable()
        local probaconflingrope = Instance.new("RopeConstraint")
        local attachmentPlr = Instance.new("Attachment", game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"))
        local attachmentTar = Instance.new("Attachment", tarplr.Character:FindFirstChild("HumanoidRootPart"))
        for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = tarplr.Character.HumanoidRootPart.CFrame
        probaconflingrope.Attachment0 = attachmentPlr
        probaconflingrope.Attachment1 = attachmentTar
        probaconflingrope.Enabled = true
        probaconflingrope.Restitution = 0
        probaconflingrope.Length = 0
        getgenv().probaconfling = game:GetService("RunService").Heartbeat:Connect(function()
            if checkavailable() then
                task.spawn(function()
                    for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                        if v:IsA("BasePart") then
                            v.CanCollide = false
                        end
                    end
                end)
                game.Players.LocalPlayer.Character.Humanoid.Sit = false
                game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,8,0)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = tarplr.Character.HumanoidRootPart.CFrame
                game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(getgenv().flingforce,getgenv().flingforce,getgenv().flingforce)
            else
                repeat
                    if game.Players.LocalPlayer.Character then
                        if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            game.Players.LocalPlayer.Character.Humanoid.Sit = false
                            game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
                            game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0,0,0)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = probaconflinglastframe
                            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                        end
                    end
                    task.wait(0.1)
                until checkavailable()
                if not attachmentPlr then
                    attachmentPlr = Instance.new("Attachment", game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"))
                end
                if not attachmentTar then
                    attachmentTar = Instance.new("Attachment", tarplr.Character:FindFirstChild("HumanoidRootPart"))
                end
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = tarplr.Character.HumanoidRootPart.CFrame
                probaconflingrope.Attachment0 = attachmentPlr
                probaconflingrope.Attachment1 = attachmentTar
                probaconflingrope.Enabled = true
                probaconflingrope.Restitution = 0
                probaconflingrope.Length = 0
            end
        end)
    else
        getgenv().probaconfling:Disconnect()
        getgenv().probaconfling = nil
        task.spawn(function()
            for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v:IsA("BasePart") then
                    v.CanCollide = true
                end
            end
        end)
        for _ = 1, 50 do
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
            game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0,0,0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = probaconflinglastframe
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
            task.wait(0.01)
        end
        repeat
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
            game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0,0,0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = probaconflinglastframe
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
            task.wait(0.5)
        until math.abs(game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity) < Vector3.new(10, 10, 10) and math.abs(game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity) < Vector3.new(10, 10, 10)
    end
end)





flingsection:NewButton("Fling Player 2.0", "", function()
    local lastcframe = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    local tarplr = GetPlayerWithClosestName(getgenv().plrtar) or GetPlayerWithClosestName(getgenv().plrtar, true)
    pcall(function()
        for _ = 1, 100 do
            task.spawn(function()
                noclip()
            end)
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,8,0)
            local tarplrpos = tarplr.Character.HumanoidRootPart.Position
            local tarplrvelo = tarplr.Character.HumanoidRootPart.Velocity--*Vector3.new(0.5,0.5,0.5)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(tarplrpos.X+tarplrvelo.X, tarplrpos.Y+tarplrvelo.Y, tarplrpos.Z+tarplrvelo.Z)
            game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(getgenv().flingforce,getgenv().flingforce,getgenv().flingforce)
            task.wait(0.01)
        end
        for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = true
            end
        end
        for _ = 1, 50 do
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
            game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0,0,0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastcframe
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
            task.wait(0.01)
        end
    end)
end)


flingsection:NewToggle("Loop Fling Player 2.0", "", function(state)
    getgenv().probaconflingplayer2 = state
    if state then
        local lastcframe = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        local tarplr = GetPlayerWithClosestName(getgenv().plrtar) or GetPlayerWithClosestName(getgenv().plrtar, true)
        pcall(function()
            local checkavailable = function()
                if tarplr then
                    if game.Players.LocalPlayer.Character and tarplr.Character then
                        if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and tarplr.Character:FindFirstChild("Humanoid") and tarplr.Character:FindFirstChild("HumanoidRootPart") then
                            if tarplr.Character:FindFirstChild("Humanoid").Health ~= 0 then
                                return true
                            end
                        end
                    end
                end
                return false
            end
            repeat
                if checkavailable() then
                    task.spawn(function()
                        noclip()
                    end)
                    game.Players.LocalPlayer.Character.Humanoid.Sit = false
                    game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
                    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,8,0)
                    local tarplrpos = tarplr.Character.HumanoidRootPart.Position
                    local tarplrvelo = tarplr.Character.HumanoidRootPart.Velocity--*Vector3.new(0.5,0.5,0.5)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(tarplrpos.X+tarplrvelo.X, tarplrpos.Y+tarplrvelo.Y, tarplrpos.Z+tarplrvelo.Z)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(getgenv().flingforce,getgenv().flingforce,getgenv().flingforce)
                    task.wait(0.01)
                else
                    repeat
                        if game.Players.LocalPlayer.Character then
                            if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                                game.Players.LocalPlayer.Character.Humanoid.Sit = false
                                game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
                                game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0,0,0)
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = probaconflinglastframe
                                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                            end
                        end
                        task.wait(0.1)
                    until checkavailable() or not getgenv().probaconflingplayer2
                end
            until not getgenv().probaconflingplayer2 or not tarplr
            for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v:IsA("BasePart") then
                    v.CanCollide = true
                end
            end
            for _ = 1, 50 do
                game.Players.LocalPlayer.Character.Humanoid.Sit = false
                game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
                game.Players.LocalPlayer.Character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0,0,0)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastcframe
                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                task.wait(0.01)
            end
        end)
    end
end)





flingsection:NewToggle("Anti Fling", "", function(state)
    if state then
        probaconantifling = game:GetService("RunService").Stepped:Connect(function()
            for _, player in pairs(game.Players:GetPlayers()) do
                if player ~= game.Players.LocalPlayer and player.Character then
                    for _, v in pairs(player.Character:GetDescendants()) do
                        if v:IsA("BasePart") then
                            v.CanCollide = false
                        end
                    end
                end
            end
        end)
    else
        probaconantifling:Disconnect()
        probaconantifling = nil
    end
end)

--Tool Tab
local ToolTab = Window:NewTab("Tool")
local ToolSection = ToolTab:NewSection("Tool")
local CheckToolSection = ToolTab:NewSection("Check Tool")
local player = game.Players.LocalPlayer

ToolSection:NewButton("TP Tool", "This tool can TP you to where you click \r Info: Click to TP \r WOW finally a useful script by Pro Bacon!", function()
    local Player = game:GetService("Players").LocalPlayer
    local Mouse = Player:GetMouse()
    local Tool = Instance.new("Tool")

    Tool.RequiresHandle = false
    Tool.Name = "TPTool"

    Tool.Activated:Connect(function()
        local Root = Player.Character.HumanoidRootPart
        local Pos = Mouse.Hit.Position+Vector3.new(0,2.5,0)
        local Offset = Pos-Root.Position
        Root.CFrame = Root.CFrame+Offset
    end)

    Tool.Parent = Player.Backpack
end)
ToolSection:NewToggle("Equip TP Tool", "In case the game has specal inventory UI", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("TPTool"))
    else
        game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Backpack:FindFirstChild("TPTool"))
    end
end)
ToolSection:NewButton("Path Tool", "This tool can print the path of an object \r Info: Click to print path \r WOW finally a useful script by Pro Bacon!", function()
    local Player = game:GetService("Players").LocalPlayer
    local Tool = Instance.new("Tool")

    Tool.RequiresHandle = false
    Tool.Name = "PathTool"

    Tool.Activated:Connect(function()
        print(game:GetService("Players").LocalPlayer:GetMouse().Target:GetFullName())
        setclipboard(game:GetService("Players").LocalPlayer:GetMouse().Target:GetFullName())
    end)

    Tool.Parent = Player.Backpack
end)
ToolSection:NewToggle("Equip Path Tool", "In case the game has specal inventory UI", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("PathTool"))
    else
        game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Backpack:FindFirstChild("PathTool"))
    end
end)
ToolSection:NewTextBox("Delete Tool", "Enter the tool name here", function(txt)
    local player = game.Players.LocalPlayer
    local ToolDelName = txt
    for i, player in pairs(game.Players:GetChildren()) do
        for i, backpack in pairs(player:GetChildren()) do
            if backpack.Name == "Backpack" then
                for i, v in pairs(backpack:GetChildren()) do
                    if v.Name == ToolDelName then
                        print("Deleting", v)
                        v:Destroy()
                    end
                end
            end
        end
    end
end)
ToolSection:NewButton("Delete holding tool", "Delete the yool you are holding.", function()
    if player.Character:FindFirstChildOfClass("Tool") then
        player.Character:FindFirstChildOfClass("Tool"):Destroy()
    end
end)

CheckToolSection:NewButton("Check Tool", "Print and notifi all tools.", function()
    for i, player in pairs(game.Players:GetChildren()) do -- players
        for i, backpack in pairs(player:GetChildren()) do
            if backpack.Name == "Backpack" then -- backpacks
                for i, v in pairs(backpack:GetChildren()) do -- stuff in backpacks
                    print(v)
                end
            end
        end
    end
end)

local TeleportTab = Window:NewTab("Teleport")
local PlayerTeleport = TeleportTab:NewSection("Players")
local CoordinateTeleport = TeleportTab:NewSection("Coordinate")
local TeleportOption = TeleportTab:NewSection("Teleport Option")
local lastplrtable = InGamePlayerNameList
local lastteleporttype, lastteleportcframe, lastteleportplayer


local ProBaconHubPlayerTeleportDropDown = PlayerTeleport:NewDropdown("Player Teleport", "Teleport to player", InGamePlayerNameList, function(PlayerOption)
    if game.Players:FindFirstChild(PlayerOption).Character then
        if game.Players:FindFirstChild(PlayerOption).Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild(PlayerOption).Character.HumanoidRootPart.CFrame
            lastteleporttype = "plr"
            lastteleportplayer = game.Players:FindFirstChild(PlayerOption)
        else
            game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Player is out of render distance, character got unloaded!"; Duration=5;})
        end
    else
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Player is out of render distance, character got unloaded!"; Duration=5;})
    end
end)
local UpdatePlayerTeleportList = function()
    repeat
        task.wait(0.01)
    until InGamePlayerNameList ~= lastplrtable
    ProBaconHubPlayerTeleportDropDown:Refresh(InGamePlayerNameList)
end
game.Players.PlayerAdded:Connect(UpdatePlayerTeleportList)
game.Players.PlayerRemoving:Connect(UpdatePlayerTeleportList)
PlayerTeleport:NewTextBox("Player", "Input player name (username/displayname)", function(txt)
    local plrfoundusername = GetPlayerWithClosestName(txt) or GetPlayerWithClosestName(txt, true)
    if plrfoundusername then
        if plrfoundusername.Character and plrfoundusername.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = plrfoundusername.Character.HumanoidRootPart.CFrame
            lastteleporttype = "plr"
            lastteleportplayer = plrfoundusername
        else
            game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Player is out of render distance, character got unloaded!"; Duration=5;})
        end
    else
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Player not found with both username and displayname!"; Duration=5;})
    end
end)

CoordinateTeleport:NewTextBox("Position", "Input position (Vector3)", function(txt)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(stringToVector3(txt))
    lastteleporttype = "cframe"
    lastteleportcframe = CFrame.new(stringToVector3(txt))
end)
CoordinateTeleport:NewTextBox("CFrame", "Input CFrame (CFrame)", function(txt)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(StringToCFrame(txt))
    lastteleporttype = "cframe"
    lastteleportcframe = CFrame.new(StringToCFrame(txt))
end)
TeleportOption:NewButton("Teleport", "Base on your previous input", function()
    if lastteleporttype == "cframe" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastteleportcframe
    elseif lastteleporttype == "plr" then
        if lastteleportplayer.Character and lastteleportplayer.Character.HumanoidRootPart then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastteleportplayer.Character.HumanoidRootPart.CFrame
        else
            game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Player is out of render distance, character got unloaded!"; Duration=5;})
        end
    end
end)

