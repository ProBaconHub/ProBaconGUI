                    getgenv().flagwarbringevent = nil
                end
            end
        end)
        flagwarsection:NewButton("Grab flag", "", function()
            for _, v in pairs(workspace.Core.Flags:GetChildren()) do
                if v.Name ~= game.Players.LocalPlayer.Team.Name and v:FindFirstChild("Flag") then
                    local pos = v.Flag.Parts.Middle.CFrame
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame*CFrame.new(0,200,0)
                    task.wait(0.1)
                    VelocityTeleport(pos.X, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y, pos.Z, 35)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos*CFrame.new(0,20,0)
                    task.wait(1)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos*CFrame.new(0,200,0)
                    task.wait(0.1)
                    pos = workspace.Core.Flags[game.Players.LocalPlayer.Team.Name].Base.CFrame
                    VelocityTeleport(pos.X, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y, pos.Z, 35)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos*CFrame.new(0,20,0)
                    break
                end
            end
        end)

        local theMeleeId = nil
        local flagwarsidcrack = nil
        flagwarsection:NewButton("Calibrate melee", "", function()
            task.spawn(function()
                theMeleeId = nil
                flagwarsidcrack = nil
                flagwarsidcrack = hookmetamethod(game, "__namecall", function(Self, ...)
                    local Args = {...}
                    local NamecallMethod = getnamecallmethod()
                    if NamecallMethod == "FireServer" and Self.Name == "ClientCast-Replication" then
                        if not theMeleeId then
                            print(Args[1])
                            theMeleeId = tonumber(Args[1])
                            print(theMeleeId)
                        end
                    end
                    return flagwarsidcrack(Self, unpack(Args))
                end)
                repeat
                    task.wait(0.1)
                until theMeleeId
            end)
        end)
        flagwarsection:NewButton("Kill All Enemies (melee)", "", function()
            if theMeleeId then
                pcall(function()
                    if not game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
                        for _, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                            if v:FindFirstChild("MeleeScript") then
                                game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
                                break
                            end
                        end
                    end
                    for _, v in pairs(game:GetService("Players"):GetPlayers()) do
                        if v.Team ~= game:GetService("Players").LocalPlayer.Team and v.Character and game:GetService("Players").LocalPlayer.Character then
                            if v.Character:FindFirstChild("HumanoidRootPart") and game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                                task.spawn(function()
                                    for _ = 1, 4 do
                                        local args = {
                                            [1] = theMeleeId,
                                            [2] = "Humanoid",
                                            [3] = {
                                                ["Instance"] = v.Character.RightUpperArm,
                                                ["Material"] = Enum.Material.SmoothPlastic,
                                                ["Normal"] = Vector3.new(0.2894057035446167, 0.4134974479675293, -0.8632866144180298),
                                                ["Position"] = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                                            }
                                        }
                                        game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("ClientCast Network"):WaitForChild("ClientCast-Replication"):FireServer(unpack(args))
                                        task.wait(0.1)
                                    end
                                end)
                            end
                        end
                    end
                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                end)
            else
                game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Haven't calibrated melee yet. \r Please swing your melee to calibrate."; Duration=5;})
            end
        end)

