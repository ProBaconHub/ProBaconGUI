local hitboxsection = PlayersTab:NewSection("Hitbox")
function teamcheck(plr)
    return not getgenv().hitboxteamcheck or (game.Players.LocalPlayer.Team and plr.Team and plr.Team == game.Players.LocalPlayer.Team)
end
getgenv().hitboxexpandpart = "HumanoidRootPart"
hitboxsection:NewDropdown("Hitbox part", "", {"Head", "HumanoidRootPart (DF)"}, function(option)

end)
hitboxsection:NewToggle("Hitbox", "", function(state)
    getgenv().hitboxexpand = state
    if getgenv().hitboxexpand then
        repeat
            for _, v in pairs(game.Players:GetPlayers()) do
                if v ~= game.Players.LocalPlayer then
                    if v.Character then
                        if v.Character:FindFirstChild("HumanoidRootPart") and v:FindFirstChildOfClass("Humanoid") and v.Character:FindFirstChild("Head") then
                            pcall(function()
                                if v:FindFirstChildOfClass("Humanoid").Health > 0 and getgenv().hitboxexpand and (not getgenv().hitboxteamcheck or not game.Players.LocalPlayer.Team or not v.Team or (v.Team ~= game.Players.LocalPlayer.Team)) then
                                    if (not getgenv().hitboxteamcheck or (not game.Players.LocalPlayer.Team or not v.Team)) then
                                        v.Character[getgenv().hitboxexpandpart].Size = Vector3.new(15, 20, 15)
                                        v.Character[getgenv().hitboxexpandpart].Transparency = 0.9
                                        v.Character[getgenv().hitboxexpandpart].BrickColor = BrickColor.new("White")
                                        v.Character[getgenv().hitboxexpandpart].Material = "Neon"
                                        v.Character[getgenv().hitboxexpandpart].CanCollide = false
                                    elseif v.Team ~= game.Players.LocalPlayer.Team then
                                        v.Character[getgenv().hitboxexpandpart].Size = Vector3.new(15, 20, 15)
                                        v.Character[getgenv().hitboxexpandpart].Transparency = 0.9
                                        v.Character[getgenv().hitboxexpandpart].BrickColor = BrickColor.new("White")
                                        v.Character[getgenv().hitboxexpandpart].Material = "Neon"
                                        v.Character[getgenv().hitboxexpandpart].CanCollide = false
                                    else
                                        v.Character[getgenv().hitboxexpandpart].Size = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].Size or Vector3.new(2, 2, 1)
                                        v.Character[getgenv().hitboxexpandpart].Transparency = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].Transparency or 1
                                        v.Character[getgenv().hitboxexpandpart].BrickColor = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].BrickColor or BrickColor.new("Medium stone grey")
                                        v.Character[getgenv().hitboxexpandpart].Material = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].Material or "Plastic"
                                        v.Character[getgenv().hitboxexpandpart].CanCollide = false
                                    end
                                else
                                    v.Character[getgenv().hitboxexpandpart].Size = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].Size or Vector3.new(2, 2, 1)
                                    v.Character[getgenv().hitboxexpandpart].Transparency = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].Transparency or 1
                                    v.Character[getgenv().hitboxexpandpart].BrickColor = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].BrickColor or BrickColor.new("Medium stone grey")
                                    v.Character[getgenv().hitboxexpandpart].Material = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].Material or "Plastic"
                                    v.Character[getgenv().hitboxexpandpart].CanCollide = false
                                end
                            end)
                        end
                    end
                end
            end
            task.wait()
        until not getgenv().hitboxexpand
        for _, v in pairs(game.Players:GetPlayers()) do
            if v ~= game.Players.LocalPlayer then
                if v.Character then
                    if v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Head") then
                        v.Character[getgenv().hitboxexpandpart].Size = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].Size or Vector3.new(2, 2, 1)
                        v.Character[getgenv().hitboxexpandpart].Transparency = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].Transparency or 1
                        v.Character[getgenv().hitboxexpandpart].BrickColor = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].BrickColor or BrickColor.new("Medium stone grey")
                        v.Character[getgenv().hitboxexpandpart].Material = game.Players.LocalPlayer.Character[getgenv().hitboxexpandpart].Material or "Plastic"
                        v.Character[getgenv().hitboxexpandpart].CanCollide = false
                    end
                end
            end
        end
    end
end)
