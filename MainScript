                for _, v in pairs(game:GetDescendants()) do
                    if v:IsA("ProximityPrompt") then
                        task.spawn(function()
                            local hb = game:GetService("RunService").Heartbeat:Connect(function()
                                v.Enabled = true
                            end)
                            repeat
                                task.wait(0.001)
                            until not enableinteractions
                            hb:Disconnect()
                        end)
                    end
                end
                notorinter = game.Workspace.DescendantAdded:Connect(function(descendant)
                    if descendant:IsA("ProximityPrompt") then
                        task.spawn(function()
                            local hb = game:GetService("RunService").Heartbeat:Connect(function()
                                descendant.Enabled = true
                            end)
                            repeat
                                task.wait(0.001)
                            until not enableinteractions
                            hb:Disconnect()
                        end)
                    end
                end)
            else
                if notorinter then
                    notorinter:Disconnect()
                    notorinter = nil
                    for _, v in pairs(game:GetDescendants()) do
                        if v:IsA("ProximityPrompt") then
                            v.Enabled = false
                        end
                    end
                end
            end
        end)
