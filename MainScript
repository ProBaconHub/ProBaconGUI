                        if not game.Players.LocalPlayer.Character then
                            break
                        end
                        LastPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                    end
                    PauseFrameLog = true
                end
            end)
            task.spawn(function()
                game.Players.LocalPlayer.CharacterAdded:Connect(function()
                    PauseFrameLog = false
                    game.Players.LocalPlayer.Character.ChildAdded:Connect(function(child)
                        if child.Name == "HumanoidRootPart" then
                            PauseFrameLog = false
                        end
                    end)
                end)
            	while task.wait(0.1) do
            	    pcall(function()
            	        if game.Players.LocalPlayer.PlayerGui.HUD.Match.GameMode.Text == "Domination" then
            	            if game.UserInputService:IsKeyDown(Enum.KeyCode.J) then
            	                repeat
            	                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map.ObjectiveHitboxes.A.CFrame
