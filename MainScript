            getgenv().MixResult = nil
            local enemylisthere = {}
            print("--------All Enemies--------")
            for i,v in pairs(workspace.Enemies:GetChildren()) do
            	if table.find(enemylisthere, v.Name) == nil then
            		table.insert(enemylisthere, v.Name)
            	end
            end
            table.sort(enemylisthere)
            for i,v in pairs(enemylisthere) do
            	print(v)
            end
            print("Done")
            game.StarterGui:SetCore("DevConsoleVisible", true)
        end)
