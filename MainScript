        destructionSection:NewButton("Nuke Area (Hold Gun)", "", function()
            local findtool = function()
                if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
                    if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Assets") then
                        return game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
                    end
                else
                    return nil
                end
            end
            if findtool() ~= nil then
                local area = game.Players.LocalPlayer.CurrentArea.Value
                if area ~= "NoZone" then
                    for i, obj in pairs(workspace.Areas[area]:GetDescendants()) do
                        if obj:IsA("BasePart") then
                            local tool = findtool()
                            local args = {
                                [1] = 1736620822.1502209,
                                [2] = tool.Stats,
                                [3] = obj.Position,
                                [4] = tool.Assets:GetChildren()[1].Boom
                            }
                            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("explodeRocket"):FireServer(unpack(args))
                            if string.sub(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StatsFrame.Stats.Bricks.amnt.Text, 1, string.find(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StatsFrame.Stats.Bricks.amnt.Text, "/")-1) == string.sub(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StatsFrame.Stats.Bricks.amnt.Text, string.find(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StatsFrame.Stats.Bricks.amnt.Text, "/")+1, string.len(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StatsFrame.Stats.Bricks.amnt.Text)) then
                                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("sellBricks"):FireServer()
                                task.wait(0.5)
                            end
                        end
                    end
                end
            else
                error("no tool", 1)
            end
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("sellBricks"):FireServer()
        end)
        destructionSection:NewToggle("Auto Sell", "", function(state)
            getgenv().destructionautosell = state
            if getgenv().destructionautosell then
                task.spawn(function()
                    repeat
                        repeat
                            if game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StoragePopupFrame.Position == UDim2.new(0,0,0,0) then
                                game:GetService("TweenService"):Create(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StoragePopupFrame, TweenInfo.new(0.1), {Position = UDim2.new(0,0,1,0)}):Play()
                            end
                            task.wait(0.1)
                        until string.sub(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StatsFrame.Stats.Bricks.amnt.Text, 1, string.find(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StatsFrame.Stats.Bricks.amnt.Text, "/")-1) == string.sub(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StatsFrame.Stats.Bricks.amnt.Text, string.find(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StatsFrame.Stats.Bricks.amnt.Text, "/")+1, string.len(game:GetService("Players").LocalPlayer.PlayerGui.MainUI.StatsFrame.Stats.Bricks.amnt.Text))
