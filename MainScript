local OtherTab = Window:NewTab("Others")
local OtherSection = OtherTab:NewSection("Others")

OtherSection:NewButton("Anti-AFK", "Prevent you from getting kicked for being idle for 20 minutes.", function()
    local cloneref = cloneref or function(service) return service end
	local VirtualUser = cloneref(game:GetService("VirtualUser"))
	game.Players.LocalPlayer.Idled:Connect(function()
		VirtualUser:CaptureController()
		VirtualUser:ClickButton2(Vector2.new())
	end)
end)
OtherSection:NewButton("Get CFrame", "Noti and copy your CFrame.", function()
    print(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
    setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame))
    game.StarterGui:SetCore("SendNotification", {
        Title = "Pro Bacon",
        Text = "Your CFrame: \n".. tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame),
        Duration = 10
    })
    game.StarterGui:SetCore("SendNotification", {Title = "Pro Bacon", Text = "Also copied to clipboard."})
end)
OtherSection:NewButton("Get Position", "Noti and copy your position.", function()
    print(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
    setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position))
    game.StarterGui:SetCore("SendNotification", {
        Title = "Pro Bacon",
        Text = "Your position: \n".. tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position),
        Duration = 10
    })
    game.StarterGui:SetCore("SendNotification", {Title = "Pro Bacon", Text = "Also copied to clipboard."})
end)
OtherSection:NewButton("Client Anti Kick", "Prevend kicking from client suck as other scripts \r wont protect from server kick!", function()
    local mt = getrawmetatable(game)
    local old = mt.__namecall
    local protect = newcclosure or protect_function

    if not protect then
    protect = function(f) return f end
    end

    setreadonly(mt, false)
    mt.__namecall = protect(function(self, ...)
    local method = getnamecallmethod()
    if method == "Kick" then
    wait(9e9)
    return
    end
    return old(self, ...)
    end)
    hookfunction(game:GetService("Players").LocalPlayer.Kick,protect(function() task.wait(9e9) end))
end)
getgenv().AutoKeyClick = false
getgenv().AutoKeyHoldTime = 0.1
getgenv().AutoKeyCCD = 0.1
task.spawn(function()
    while true do
        repeat
            task.wait(.0001)
        until getgenv().AutoKeyClick
        repeat
            pcall(function()
                game:GetService('VirtualInputManager'):SendKeyEvent(true, getgenv().AutoKeyToClick, false, game)
                task.wait(getgenv().AutoKeyHoldTime)
                game:GetService('VirtualInputManager'):SendKeyEvent(false, getgenv().AutoKeyToClick, false, game)
                task.wait(getgenv().AutoKeyCCD)
            end)
        until not getgenv().AutoKeyClick
    end
end)
local enumlist = {}
for _, v in pairs(Enum.KeyCode:GetEnumItems()) do
    table.insert(enumlist, v.Name)
end
OtherSection:NewDropdown("Auto Key Press", "Spam key for you.", enumlist, function(currentOption)
    getgenv().AutoKeyToClick = Enum.KeyCode[currentOption]
end)
OtherSection:NewTextBox("Key Press & Hold (s)", "How long the key will hold in seconds", function(txt)
    txt = tonumber(txt)
    getgenv().AutoKeyHoldTime = txt
end)
OtherSection:NewTextBox("Press CD", "How many seconds to wait after pressing again?", function(txt)
    txt = tonumber(txt)
    if txt ~= 0 then
        getgenv().AutoKeyCCD = txt
    else
        getgenv().AutoKeyCCD = 1
    end
end)
OtherSection:NewToggle("Auto Key Click", "Base on the above settings.", function(state)
    getgenv().AutoKeyClick = state
end)
OtherSection:NewButton("Click once", "Auto click once", function()
    game:GetService('VirtualInputManager'):SendKeyEvent(true, getgenv().AutoKeyClick, false, game)
    task.wait(getgenv().AutoKeyHoldTime)
    game:GetService('VirtualInputManager'):SendKeyEvent(false, getgenv().AutoKeyClick, false, game)
end)

