        CityLifeCook:NewToggle("Auto Cook", "Auto cook and send to costomer", function(state)
            getgenv().CityLifeAutoCook = state
            local FindHoldingItem = function(cooked)
                for _, v in pairs(workspace:GetChildren()) do
                    if v:FindFirstChild("stages") and v:FindFirstChild("Plate") and v:FindFirstChild("height") and v:FindFirstChild("prim") then
                        if v.prim:FindFirstChild("LeftHand->prim") then
            				if v.prim["LeftHand->prim"].Part1 == game.Players.LocalPlayer.Character.LeftHand then
                        	    if cooked then
                        	        if #v.stages:GetChildren() ~= 0 then
                        	            return v
                        	        end
                        	    else
                        	        if #v.stages:GetChildren() == 0 then
                        	            return v
                        	        end
                        	    end
                        	end
            			end
                    end
                end
                return nil
            end
            local FindCookingSpotWithItem = function()
                for _, TargetCookSpot in pairs(workspace.jobInstances.cook.CookSpots:GetChildren()) do
                    if TargetCookSpot:FindFirstChildWhichIsA("Model") then
                        return true
                    end
                end
                return false
            end
            local GetEmptyDish = function()
                for _, TargetOrderSpot in pairs(workspace.jobInstances.cook.OrderSpots:GetChildren()) do
                    if not getgenv().CityLifeAutoCook then
                        break
                    end
                    if TargetOrderSpot:GetChildren() then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = TargetOrderSpot.CFrame
                        task.wait(0.1)
                        game:GetService('VirtualInputManager'):SendKeyEvent(true, Enum.KeyCode.E, false, game)
                        task.wait(0.01)
                        game:GetService('VirtualInputManager'):SendKeyEvent(false, Enum.KeyCode.E, false, game)
                        break
                    end
                end
            end
            local PlaceEmptyDishOnCookingSpot = function()
                repeat
                    for _, TargetCookSpot in pairs(workspace.jobInstances.cook.CookSpots:GetChildren()) do
                        if not TargetCookSpot:FindFirstChildWhichIsA("Model") and TargetCookSpot.prompt.Enabled then
                            repeat
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = TargetCookSpot.CFrame
                                task.wait(0.1)
                                game:GetService('VirtualInputManager'):SendKeyEvent(true, Enum.KeyCode.E, false, game)
                                task.wait(0.01)
                                game:GetService('VirtualInputManager'):SendKeyEvent(false, Enum.KeyCode.E, false, game)
                            until TargetCookSpot:FindFirstChildWhichIsA("Model") or not TargetCookSpot.prompt.Enabled or not getgenv().CityLifeAutoCook
                            break
                        end
                    end
                    task.wait(0.001)
                until not FindHoldingItem(true) or not getgenv().CityLifeAutoCook
            end
            local GetFoodCooked = function()
                while task.wait(0.00001) do
                    if not getgenv().CityLifeAutoCook then
                        break
                    end
                    if workspace.jobInstances.cook.CookSpots:GetChildren()[1]:FindFirstChildWhichIsA("Model") then
                        if workspace.jobInstances.cook.CookSpots:GetChildren()[1]:FindFirstChildWhichIsA("Model").prim:FindFirstChild("plate") then
                            break
                        end
                        task.spawn(function()
                            game:GetService("ReplicatedStorage").remoteInterface.jobs.action:InvokeServer(workspace.jobInstances.cook.CookSpots:GetChildren()[1])
                        end)
                        task.wait(0.25)
                    elseif workspace.jobInstances.cook.CookSpots:GetChildren()[2]:FindFirstChildWhichIsA("Model") then
                        if workspace.jobInstances.cook.CookSpots:GetChildren()[2]:FindFirstChildWhichIsA("Model").prim:FindFirstChild("plate") then
                            break
                        end
                        task.spawn(function()
                            game:GetService("ReplicatedStorage").remoteInterface.jobs.action:InvokeServer(workspace.jobInstances.cook.CookSpots:GetChildren()[2])
                        end)
                        task.wait(0.25)
                    elseif workspace.jobInstances.cook.CookSpots:GetChildren()[3]:FindFirstChildWhichIsA("Model") then
                        if workspace.jobInstances.cook.CookSpots:GetChildren()[3]:FindFirstChildWhichIsA("Model").prim:FindFirstChild("plate")then
                            break
                        end
                        task.spawn(function()
                            game:GetService("ReplicatedStorage").remoteInterface.jobs.action:InvokeServer(workspace.jobInstances.cook.CookSpots:GetChildren()[3])
                        end)
                        task.wait(0.25)
                    elseif workspace.jobInstances.cook.CookSpots:GetChildren()[4]:FindFirstChildWhichIsA("Model") then
                        if workspace.jobInstances.cook.CookSpots:GetChildren()[4]:FindFirstChildWhichIsA("Model").prim:FindFirstChild("plate") then
                            break
                        end
                        task.spawn(function()
                            game:GetService("ReplicatedStorage").remoteInterface.jobs.action:InvokeServer(workspace.jobInstances.cook.CookSpots:GetChildren()[4])
                        end)
                        task.wait(0.25)
                    elseif workspace.jobInstances.cook.CookSpots:GetChildren()[5]:FindFirstChildWhichIsA("Model") then
                        if workspace.jobInstances.cook.CookSpots:GetChildren()[5]:FindFirstChildWhichIsA("Model").prim:FindFirstChild("plate") then
                            break
                        end
                        task.spawn(function()
                            game:GetService("ReplicatedStorage").remoteInterface.jobs.action:InvokeServer(workspace.jobInstances.cook.CookSpots:GetChildren()[5])
                        end)
                        task.wait(0.25)
                    else
                        break
                    end
                end
                for _,v in pairs(workspace.jobInstances.cook.CookSpots:GetChildren()) do
                    if v:FindFirstChildWhichIsA("Model") then
                        if v:FindFirstChildWhichIsA("Model").prim:FindFirstChild("plate") then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                            task.wait(0.1)
                        end
                    end
                end
                game:GetService('VirtualInputManager'):SendKeyEvent(true, Enum.KeyCode.E, false, game)
                task.wait(0.01)
                game:GetService('VirtualInputManager'):SendKeyEvent(false, Enum.KeyCode.E, false, game)
            end
            local FindCustomerForCookingJob = function()
                for i, v in pairs(workspace.jobInstances.cook.CustomerSpots:GetChildren()) do
                    if v:FindFirstChild("customer") then
                        if v:FindFirstChild("customer").Enabled then
                            return v
                        end
                    end
                end
                for i, v in pairs(workspace.jobInstances.cook.DeliverNodes:GetChildren()) do
                    if v:FindFirstChild("customer") then
                        if v:FindFirstChild("customer").Enabled then
                            return v
                        end
                    end
                end
                return nil
            end

            if getgenv().CityLifeAutoCook then
                local LastCFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                print(FindHoldingItem(true))
                repeat
                    if not FindHoldingItem(true) then --Not holding cooked item
                        if not FindCookingSpotWithItem() then
                            if not FindHoldingItem(false) then
                                for _,_ in pairs(workspace.jobInstances.cook.OrderSpots:GetChildren()) do
                                    GetEmptyDish()
                                    PlaceEmptyDishOnCookingSpot()
                                end
                            else
                                PlaceEmptyDishOnCookingSpot()
                            end
                        end
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =  CFrame.new(-77, 4, -807)
                        GetFoodCooked()
                    end
                    if not FindHoldingItem(false) and FindHoldingItem(true) then --Holding cooked dish, not holding empty dish
                        if FindCustomerForCookingJob() ~= nil then
                            repeat
                                local customer = FindCustomerForCookingJob()
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = customer.CFrame
                                task.wait(0.25)
                                game:GetService('VirtualInputManager'):SendKeyEvent(true, Enum.KeyCode.E, false, game)
                                task.wait(0.1)
                                game:GetService('VirtualInputManager'):SendKeyEvent(false, Enum.KeyCode.E, false, game)
                                task.wait(0.01)
                                customer:FindFirstChild("customer").Enabled = false
                            until FindCustomerForCookingJob() == nil or not getgenv().CityLifeAutoCook
                        end
                    end
                    task.wait(0.01)
                until not getgenv().CityLifeAutoCook
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = LastCFrame
            end
        end)
        CityLifeHair:NewButton("Auto Cut Hair", "Only cut hair", function()
            local LastCFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            for i,v in pairs(workspace.jobInstances.hairsalon:GetChildren())do
            	if v.Name ~= "NPCNodes" and #v:GetChildren() == 2 then
            		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
            		task.wait(0.001)
                    for _ = 1, 5 do
                        task.spawn(function()
                            game:GetService("ReplicatedStorage").remoteInterface.jobs.action:InvokeServer(v)
                        end)
                        task.wait(0.5)
                    end
            	end
            end
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = LastCFrame
        end)
    
    
    
    
    
    elseif Tab == "Mad City CH1" then
        function isincrimbase(playername)
            player = game.Players[playername]
            character = player.character
            lp = game.Players.LocalPlayer.Character
            if character then
                humanoidRootPart = character.HumanoidRootPart
            else
                return true
            end
            local raycastParams = RaycastParams.new()
            raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
            raycastParams.FilterDescendantsInstances = {lp, character, workspace.Ignore} -- Exclude the player's character from the raycast
            local raycastResult = workspace:Raycast(
                humanoidRootPart.Position,
                Vector3.new(0, 1000, 0),
                raycastParams
            )
            l = false
            if raycastResult then
                local hitPart = raycastResult.Instance
                for i, v in pairs(workspace.CriminalBase1:GetDescendants()) do
                    if v.Name == hitPart.Name then
                        l = true
                        break
                    else
                        l = false
                    end
                end
            end
            return l
        end
        function CheckArrestable(Player)
            if Player.Team.Name == "Criminals" or Player.Team.Name == "Villains" and not isincrimbase(Player) then
                return true
            else
                return false
            end
        end
        getgenv().BypassMethod = "Tween"
        function BypassTP(x, y, z, tpmethod)
            if tpmethod == nil then
                tpmethod = getgenv().BypassMethod
            end
            if tpmethod == "Tween" then
                VelocityTeleport(x, y, z, 1500)
            end
            if tpmethod == "Instant" then
                game:GetService("Players").LocalPlayer.CriminalMarker.Value = false
                local tpbypass = workspace.Pyramid.Tele.Core2
                tpbypass.Transparency, tpbypass.CanCollide = 1, false
                tpbypass.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                task.wait()
                workspace.Pyramid.Tele.Core2.CFrame = CFrame.new(1231.141845703125, 51051.234375, 381.09619140625)
                tpbypass.Transparency, tpbypass.CanCollide = 0, true
            end
            for _ = 1, 50 do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(x, y, z)
                task.wait()
            end
        end
        function Touchy(path, t)
            local pos = path.CFrame
            local i, m = 0
            if t == nil then
                m = 1
            else
                m = t
            end
            repeat
                path.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                task.wait(0.1)
                i = i + 0.1
            until i >= m
            path.CFrame = pos
            --firetouchinterest(path,game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("BasePart"),0)
            --firetouchinterest(path,game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("BasePart"),1)
        end

        local CheckStuff = Window:NewTab("Checking smth")
        local CheckBypass = CheckStuff:NewSection("Checking bypass method...")
        local checkpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        task.spawn(function()
            BypassTP(checkpos.X, checkpos.Y, checkpos.Z, "Instant")
        end)
        repeat
            task.wait(.01)
        until game.Players.LocalPlayer.PlayerGui.MainGUI.TeleportEffect.Transparency ~= 1
        repeat
            task.wait(.01)
        until game.Players.LocalPlayer.PlayerGui.MainGUI.TeleportEffect.Transparency == 1
        task.wait(.5)
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-Vector3.new(-1048, 18, -492)).Magnitude > 5 then
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
            getgenv().BypassMethod = "Instant"
            game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Teleportation bypass available!"; Duration=5;})
        else
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
            BypassTP(checkpos.X, checkpos.Y, checkpos.Z, "Tween")
        end
        for _,v in pairs(game.CoreGui:GetDescendants()) do
            if v:IsA("TextButton") then
                if v.Name == "Checking smthTabButton" then
                    v:Destroy()
                end
            end
            if v:IsA("TextLabel") then
                if v.Text == "Checking bypass method..." then
                    v.Text = "Done!"
                end
            end
        end

