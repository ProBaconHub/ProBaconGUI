        local lotbsx, lotbsy, lotbsz
        local function ChangeHitBoxlotbs(x,y,z)
            for _, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if v:FindFirstChild("Handle") then
                    v.Handle.Size = Vector3.new(x, y, z)
                    v.Handle.Transparency = 0.9
                end
            end
            for _, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v:IsA("Tool") and v:FindFirstChild("Handle") then
                    v.Handle.CanCollide = false
                    v.Handle.Size = Vector3.new(x, y, z)
                    v.Handle.Transparency = 0.9
                end
            end
        end
        LotbsWeapoSection:NewSlider("Hitbox.X", "Change the hitbox", 1000, 1, function(var)
            lotbsx = var
            ChangeHitBoxlotbs(lotbsx, lotbsy, lotbsz)
        end)
        LotbsWeapoSection:NewSlider("Hitbox.Y", "Change the hitbox", 1000, 1, function(var)
            lotbsy = var
            ChangeHitBoxlotbs(lotbsx, lotbsy, lotbsz)
        end)
        LotbsWeapoSection:NewSlider("Hitbox.Z", "Change the hitbox", 1000, 1, function(var)
            lotbsz = var
            ChangeHitBoxlotbs(lotbsx, lotbsy, lotbsz)
        end)
        LotbsWeapoSection:NewTextBox("Size", "Input Vector3", function(txt)
            local vector3here = stringToVector3(txt)
            print(vector3here)
            lotbsx = vector3here.X
            lotbsy = vector3here.Y
            lotbsz = vector3here.Z
            ChangeHitBoxlotbs(vector3here.X, vector3here.Y, vector3here.Z)
        end)
