--Settings

local SettingTab = Window:NewTab("Settings")
local SettingSection = SettingTab:NewSection("Setting")

SettingSection:NewKeybind("Toggle GUI", "The input key will be the key to toggle GUI", Enum.KeyCode.Delete, function(state)
    if state then
	    Library:ToggleUI()
    end
end)

SettingSection:NewButton("Check for update", "Check for new update", function()
    game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Checking for updates..."; Duration=5;})
    local UpdateDaFunctions = function(Input)
        print('Input')
        if Input == "Yes" then
            game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Functions will be updated."; Duration=5;})
            UpdateFunctions()
            game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Functions has been updated!"; Duration=5;})
        elseif Input == "No" then
            game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Functions will be updated automatically next time you execute!"; Duration=5;})
        end
    end
    local Bindable = Instance.new("BindableFunction")
    Bindable.OnInvoke = UpdateDaFunctions
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ProBaconHub/ProBaconGUI/main/Version%20Control?TheVersionOrCache="..math.random(), true))()
    if SelfVersion ~= _G.GuiVersion then
        game.StarterGui:SetCore("SendNotification", {
            Title="Pro Bacon";
            Text="Gui is outdated! Please re-execute with the launcher to get the latest update.";
            Duration=5;
        })
    elseif SelfFunctionVersion ~= _G.FunctionVersion then
        game.StarterGui:SetCore("SendNotification", {
            Title="Pro Bacon";
            Text="Functions are outdated! Do you want to update now?.";
            CallBack = Bindable;
            Button1 = "Yes";
            Button2 = "No";
            Duration = 5;
        })
    else
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="This script is up to date!"; Duration=5;})
        game.StarterGui:SetCore("SendNotification", {
            Title="Pro Bacon";
            Text="Functions are outdated! Do you want to update now?.";
            CallBack = Bindable;
            Button1 = "Yes";
            Button2 = "No";
            Duration = 5;
        })
    end
end)
SettingSection:NewButton("Check Version", "Check Version", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ProBaconHub/ProBaconGUI/main/Version%20Control?TheVersionOrCache="..math.random(), true))()
    warn("--------[[GUI STATE]]--------")
    print("Current version of GUI: ", SelfVersion, ("\n"), "Available version of GUI: ", _G.GuiVersion, ("\n"):rep(2), "Current version of function: ", SelfFunctionVersion, ("\n"), "Available version of function: ", _G.FunctionVersion)
    game.StarterGui:SetCore("DevConsoleVisible", true)
end)


--New Tab Function
function AddTab(Tab)
    if Tab == "Adopt Me!" then
        --Adopt
        local AdoptMeTab = Window:NewTab("Adopt Me!")
        local AdoptMeFarmSection = AdoptMeTab:NewSection("Auto Farm")
        local PetSection = AdoptMeTab:NewSection("Pet")

        AdoptMeFarmSection:NewButton("Dev finding for Baby Auto Farm", "Voluntear to help Pro Bacon", function()
            clipboardCopy("Discord invite link")
        end)

        PetSection:NewButton("Dev finding for Neon Pet", "Voluntear to help Pro Bacon", function()
            clipboardCopy("Discord invite link")
        end)
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})


    elseif Tab == "Build a Boat" then
        local BuildaBoatTab = Window:NewTab("Build a Boat")
        local BABFTFarmSection = BuildaBoatTab:NewSection("Auto Farm")
        local BABFTTPSection = BuildaBoatTab:NewSection("Teletort")
        
        BABFTFarmSection:NewToggle("Auto Gold", "Earn gold fast", function(state)
            getgenv().BABFTgoldfarmloop = state
            function BABFTgoldTeleport(x,y,z,cd)
                if getgenv().BABFTgoldfarmloop then
                    if game.Players.LocalPlayer.Character:FindFirstChild("Head") and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        local PlayerRoot = game.Players.LocalPlayer.Character.HumanoidRootPart
                        PlayerRoot.Anchored = false
                        PlayerRoot.CFrame = CFrame.new(x,y,z)
                        local Plat = Instance.new("Part", game.Workspace)
                        Plat.Anchored = true
                        Plat.Transparency = 1
                        Plat.Size = Vector3.new(10, 1, 10)
                        Plat.CFrame = CFrame.new(PlayerRoot.CFrame.X, PlayerRoot.CFrame.Y-3, PlayerRoot.CFrame.Z)
                        task.wait(cd)
                        Plat:Destroy()
                    end
                else
                    game.Players.LocalPlayer.Character:BreakJoints()
                end
            end
            task.spawn(function()
                if getgenv().BABFTgoldfarmloop then
                    repeat
                        repeat task.wait(0.001) until game.Players.LocalPlayer.Character:FindFirstChild("Head")
                        game.Workspace.ClaimRiverResultsGold:FireServer()
                        task.wait(0.001)
                    until not getgenv().BABFTgoldfarmloop
                end
            end)
            if getgenv().BABFTgoldfarmloop then
                repeat
                    if game.Players.LocalPlayer.Character:FindFirstChild("Head") and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        BABFTgoldTeleport(-42, 40, 1254, 6)
                        BABFTgoldTeleport(-45, 40, 1518, 2)
                        BABFTgoldTeleport(-58, 40, 2310, 2)
                        BABFTgoldTeleport(-60, 40, 3046, 2)
                        BABFTgoldTeleport(-52, 40, 3829, 2)
                        BABFTgoldTeleport(-37, 40, 4490, 2)
                        BABFTgoldTeleport(-64, 40, 5324, 2)
                        BABFTgoldTeleport(-53, 40, 6078, 2)
                        BABFTgoldTeleport(-54, 40, 6830, 2)
                        BABFTgoldTeleport(-45, 105, 7702, 2)
                        game.Players.LocalPlayer.Character:BreakJoints()
                    else
                        task.wait(0.001)
                    end
                until not getgenv().BABFTgoldfarmloop
            end
        end)
        BABFTFarmSection:NewButton("Gold block autofarm (Beta)", "Click \"Claim\" to end farm", function()
            if game.Players.LocalPlayer.Character:FindFirstChild("Head") and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                if getgenv().AutoTriggerTheChest ~= nil then
                    getgenv().AutoTriggerTheChest:Disconnect()
                end
                local GoldBlockutoFarm = true
                print("[ProBaconHub]: Gold Auto Farm | Triggering checkpoint...")
                game.Workspace.ClaimRiverResultsGold:FireServer()
                task.wait(0.1)
                local LastCFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                local PlayerRoot = game.Players.LocalPlayer.Character.HumanoidRootPart
                PlayerRoot.Anchored = false
                PlayerRoot.CFrame = CFrame.new(-45, 75, 1450)
                local Plat = Instance.new("Part", game.Workspace)
                Plat.Anchored = true
                Plat.Transparency = 1
                Plat.Size = Vector3.new(10, 1, 10)
                Plat.CFrame = CFrame.new(PlayerRoot.CFrame.X, PlayerRoot.CFrame.Y-3, PlayerRoot.CFrame.Z)
                task.wait(5)
                Plat:Destroy()
                PlayerRoot.CFrame = LastCFrame
                task.wait(0.1)
                game.Workspace.BoatStages.NormalStages.TheEnd.GoldenChest.Trigger.CFrame = PlayerRoot.CFrame
                task.wait(1)
                game.Workspace.BoatStages.NormalStages.TheEnd.GoldenChest.Trigger.CFrame = CFrame.new(-55.7065125, -358.739624, 9492.35645, 0, 0, -1, 0, 1, 0, 1, 0, 0)
                local WaitForChestRespond = 0
                repeat
                    task.wait(0.1)
                    WaitForChestRespond = WaitForChestRespond + 0.1
                until game.Players.LocalPlayer.PlayerGui:FindFirstChild("NoChestAnimation") or WaitForChestRespond > 15
                if WaitForChestRespond > 10 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-56, -339, 9486)
                end
                getgenv().AutoTriggerTheChest = game.Players.LocalPlayer.CharacterAdded:Connect(function()
                    local SuccessfullyGottenGold = false
                    repeat
                        if not game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            repeat
                                task.wait(0.000001)
                            until game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                        end
                        print("[ProBaconHub]: Gold Auto Farm | Bypassing...")
                        task.wait(1)
                        game.Workspace.BoatStages.NormalStages.TheEnd.GoldenChest.Trigger.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                        print("[ProBaconHub]: Gold Auto Farm | Spoofing start interact with chest...")
                        task.wait(0.5)
                        game.Workspace.BoatStages.NormalStages.TheEnd.GoldenChest.Trigger.CFrame = CFrame.new(-55.7065125, -358.739624, 9492.35645, 0, 0, -1, 0, 1, 0, 1, 0, 0)
                        print("[ProBaconHub]: Gold Auto Farm | Spoofing end interact with chest...")
                        local WaitForChestRespond = 0
                        repeat
                            task.wait(0.1)
                            WaitForChestRespond = WaitForChestRespond + 0.1
                        until game.Players.LocalPlayer.PlayerGui:FindFirstChild("NoChestAnimation") or WaitForChestRespond > 15
                        if WaitForChestRespond < 10 then
                            SuccessfullyGottenGold = true
                        else
                            task.wait(5)
                        end
                    until SuccessfullyGottenGold or not GoldBlockutoFarm
                end)
                repeat
                    task.wait(0.0001)
                until not game.Players.LocalPlayer.PlayerGui:FindFirstChild("RiverResultsGui")
                repeat
                    task.wait(0.0001)
                until game.Players.LocalPlayer.PlayerGui:FindFirstChild("RiverResultsGui") and not game.Players.LocalPlayer.PlayerGui.RiverResultsGui.Frame.Visible
                getgenv().AutoTriggerTheChest:Disconnect()
                GoldBlockutoFarm = false
                print("Done")
            end
        end)

        local BuildABoatPlot = {}
        for _,v in pairs(game.Workspace.Teams:GetChildren()) do
            if v:FindFirstChild("FlagPole") then
                table.insert(BuildABoatPlot, v.Name)
            end
        end
        local BuildABoatPlayerTeleportDropDown = BABFTTPSection:NewDropdown("Player Teleport", "Teleport to player", InGamePlayerNameList, function(PlayerOption)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild(PlayerOption).Character.HumanoidRootPart.CFrame
        end)
        local UpdatePlayerTeleportList = function()
            task.wait(1)
            BuildABoatPlayerTeleportDropDown:Refresh(InGamePlayerNameList)
        end
        game.Players.PlayerAdded:Connect(UpdatePlayerTeleportList)
        game.Players.PlayerRemoving:Connect(UpdatePlayerTeleportList)
        BABFTTPSection:NewDropdown("Plot Teleport", "Teleport to team plot", BuildABoatPlot, function(PlotOption)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Teams:FindFirstChild(PlotOption).Baseplate.CFrame*CFrame.new(0,3,60)
        end)
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})
    elseif Tab == "Punch Wall Simulator" then
        --Pws
        local PunchWallSimTab = Window:NewTab("PunchWallSim")
        local PwsCashSection = PunchWallSimTab:NewSection("Cash")
        local PwsPunchSection = PunchWallSimTab:NewSection("Punch")
        local PwsGiftSection = PunchWallSimTab:NewSection("Gift")
        local PwsSpinSection = PunchWallSimTab:NewSection("Spin")
        local PwsTipSection = PunchWallSimTab:NewSection("Glitch Tip")
        local PwsFarmSection = PunchWallSimTab:NewSection("Farm")
        getgenv().PwsFarmMode = "Normal"
        getgenv().PwsFarmEnable = false

        PwsCashSection:NewButton("Gimmi Cash!", "Give you money by compleating lvl 10", function()
            game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        end)
        PwsPunchSection:NewButton("Gimmi Punch!", "Give you more punch!", function()
            game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        end)
        PwsGiftSection:NewDropdown("Get Gift", "Choose what gift u wanna get", {1, 2, 3, 4, 5, 6, 7, 8}, function(currentOption)
            game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(currentOption)
        end)
        PwsGiftSection:NewButton("Clam All Gift! (OP)", "Clam all gift without getting them, can do many times By: Pro Bacon", function()
            for i = 1, 8 do
                game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(i)
                wait()
            end
        end)
        PwsSpinSection:NewDropdown("Get Spin", "Choose what prise u wanna get", {1, 2, 3, 4, 5}, function(currentOption)
            game:GetService("ReplicatedStorage").Event.Spin:FireServer(currentOption)
        end)
        PwsSpinSection:NewButton("Get All Spin! (OP)", "Get all spin By: Pro Bacon", function()
            for i = 1, 5 do
                game:GetService("ReplicatedStorage").Event.Spin:FireServer(i)
                wait()
            end
        end)
        PwsTipSection:NewButton("Get Tip", "Wot tip?", function()
            game.StarterGui:SetCore("SendNotification", {
                Icon = " ",
                 Title = "Pro Bacon",
                 Text = "Run while jump can step on your pet"
            })
            game.StarterGui:SetCore("SendNotification", {
                Icon = " ",
                 Title = "Pro Bacon",
                 Text = "It will make you jump higher and faster"
            })
        end)
        PwsFarmSection:NewButton("Gimmi All! (OP)", "By: Pro Bacon", function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/ProBaconHub/ProBaconGUI/main/PwsFarmProBaconBestMode", true))()
        end)
