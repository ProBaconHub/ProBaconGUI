    elseif Tab == "Cart Ride Into Rdite" then
        local cartrideTab = Window:NewTab("Cart Ride Into Rdite")
        local cartrideSectioon = cartrideTab:NewSection("Cart Ride Into Rdite", true)
        getgenv().setspeed = 0
        getgenv().carOn = false
        cartrideSectioon:NewTextBox("Cart Speed", "", function(txt)
            getgenv().setspeed = tonumber(txt)
        end)
        cartrideSectioon:NewToggle("Car state", "", function(state)
            getgenv().carOn = state
        end)
        cartrideSectioon:NewButton("Apply value to all carts", "", function()
            if not getgenv().saftylock then
                game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text='Starting'; Duration=5;})
                getgenv().saftylock = true
                for _,v in pairs(workspace:GetDescendants()) do
                    if v.Name == "On" and v:FindFirstChild("Click") then
                        if getgenv().setspeed > v.Parent.Configuration.Speed.Value then
                            for _ = 1, (getgenv().setspeed - v.Parent.Configuration.Speed.Value)/5 do
                                fireclickdetector(v.Parent.Up.Click)
                            end
                        elseif getgenv().setspeed < v.Parent.Configuration.Speed.Value then
                            for _ = 1, (v.Parent.Configuration.Speed.Value - getgenv().setspeed)/5 do
                                fireclickdetector(v.Parent.Down.Click)
                            end
                        end
                        if v.Parent.Configuration.CarOn.Value ~= getgenv().carOn then
                            fireclickdetector(v.Click)
                        end
                    end
                end
                getgenv().saftylock = false
                game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text='Car speed set to: '..getgenv().setspeed..'\n and state to: '..tostring(getgenv().carOn); Duration=5;})
            end
        end)
        cartrideSectioon:NewButton("Fix", "fix if apply doesnt work anymore", function()
            getgenv().saftylock = false
        end)
        cartrideSectioon:NewToggle("Spam buttons [Laggy]", "", function(state)
            getgenv().spamcart = state
            if state then
                task.spawn(function()
                    repeat
                        for _,v in pairs(workspace:GetDescendants()) do
                            if v.Name == "Click" then
                                if v.Parent.Name == "On" and v.Parent.Parent:FindFirstChild("Configuration") then
                                    if not v.Parent.Parent.Configuration.Speed.Value then
                                        fireclickdetector(v)
                                    end
                                end
                                fireclickdetector(v)
                            end
                        end
                        task.wait(0.1)
                    until not getgenv().spamcart
                end)
            end
        end)
