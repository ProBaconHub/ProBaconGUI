    elseif Tab == "Fisch" then --game:GetService("ReplicatedStorage").packages.Net["RE/DailyReward/Claim"]:FireServer()
        local fischTab = Window:NewTab("Fisch")
        local fishSec = fischTab:NewSection("Fisch")
        getgenv().fischfarmcd = 3
        getgenv().fischfarmlockplr = true
        fishSec:NewToggle("Auto Fish", "self explanatory (Stand next to the water/sea you want to fish at.", function(state)
            getgenv().fischfarm = state
            local findrod = function(path)
                for i,v in pairs(path:GetChildren()) do
                    if v:FindFirstChild("rod/client") then
                        return v
                    end
                end
                return nil
            end
            if getgenv().fischfarm then
                local lastplrpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position
                task.spawn(function()
                    repeat
                        if getgenv().fischfarmlockplr then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.Position = lastplrpos
                        end
                        task.wait(0.0001)
                    until not getgenv().fischfarm
                end)
                if findrod(game.Players.LocalPlayer.Backpack) then
                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(findrod(game.Players.LocalPlayer.Backpack))
                end
                if findrod(game.Players.LocalPlayer.Character) then
                    local rod = findrod(game.Players.LocalPlayer.Character)
                    repeat
                        if getgenv().fischfarm then
                            repeat
                                if findrod(game.Players.LocalPlayer.Backpack) then
                                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(findrod(game.Players.LocalPlayer.Backpack))
                                end
                                rod = findrod(game.Players.LocalPlayer.Character)
                                if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("shakeui") then
                                    if game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone:FindFirstChild("button") then
                                        local tar = game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone:FindFirstChild("button")
                                        tar.AnchorPoint = Vector2.new(0.5,0.5)
                                        local Vector = Vector2.new(workspace.CurrentCamera.ViewportSize.X / 2, workspace.CurrentCamera.ViewportSize.Y / 2  - (game:GetService("GuiService"):GetGuiInset().Y/2))
                                        tar.Parent.Position = UDim2.new(0, Vector.X, 0, Vector.Y)
                                        tar.Position = UDim2.new(0.5,0,0.5,0)
                                        task.wait()
                                        game:GetService('VirtualInputManager'):SendMouseButtonEvent(Vector.X, Vector.Y, 0, true, game, 1)
                                        game:GetService('VirtualInputManager'):SendMouseButtonEvent(Vector.X, Vector.Y, 0, false, game, 1)
                                    end
                                else
                                    if rod:FindFirstChild("bobber") and rod.values.bobberzone.Value == "nil" then
                                        findrod(game.Players.LocalPlayer.Character).events.reset:FireServer()
                                    end
                                    if not rod:FindFirstChild("bobber") then
                                        repeat
                                            rod.events.cast:FireServer(1000, 1)
                                            task.wait(.0001)
                                        until rod:FindFirstChild("bobber") or not getgenv().fischfarm
                                        local fischwaittime = 0
                                        repeat
                                            task.wait(0.1)
                                            fischwaittime = fischwaittime + 0.1
                                        until fischwaittime >= 1 or game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("shakeui")
                                    end
                                end
                                task.wait(0.00001)
                            until rod.values.bite.Value or not getgenv().fischfarm
                        end
                        if getgenv().fischfarm and rod.values.bite.Value then
                            repeat
                                game:GetService("ReplicatedStorage"):WaitForChild("events"):WaitForChild("reelfinished"):FireServer(1000, true)
                                task.wait(0.0001)
                            until (game:GetService("Players").LocalPlayer.PlayerGui.hud.safezone.backpack.Visible and not game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("shakeui")) or not getgenv().fischfarm
                        end
                    until not getgenv().fischfarm
                end
            else
                if findrod(game.Players.LocalPlayer.Character) then
                    if findrod(game.Players.LocalPlayer.Character).values.bite.Value then
                        repeat
                            game:GetService("ReplicatedStorage"):WaitForChild("events"):WaitForChild("reelfinished"):FireServer(100, true)
                            task.wait(0.0001)
                        until game:GetService("Players").LocalPlayer.PlayerGui.hud.safezone.backpack.Visible and not game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("shakeui")
                    end
                    findrod(game.Players.LocalPlayer.Character).events.reset:FireServer()
                    task.wait()
                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                end
            end
        end)
