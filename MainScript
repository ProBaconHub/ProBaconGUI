        MadAuto:NewButton("Auto Arrest (Police)", "Arrest Crimnal and Villans", function()
            function CheckArrestable(Player, prisoners)
                if prisoners then
                    if Player.Team.Name == "Criminals" or Player.Team.Name == "Villains" then
                        return true
                    else
                        return false
                    end
                else
                    if Player.Team.Name == "Criminals" or Player.Team.Name == "Villains" then
                        return true
                    else
                        return false
                    end
                end
            end
            game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunction"):InvokeServer("SetTeam", "Prisoners")
            repeat
                task.wait(1)
            until game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunction"):InvokeServer("SetTeam", "Police")
            repeat
                task.wait(1)
            until game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            if game.Players.LocalPlayer:FindFirstChild("LowerTorso") then
                game.Players.LocalPlayer.Character.LowerTorso:Destroy()
            end
            for i, v in pairs(game.Players:GetPlayers()) do
                if game.Players.LocalPlayer.PlayerGui.MainGUI:FindFirstChild("TeleportEffect") then
                    game.Players.LocalPlayer.PlayerGui.MainGUI.TeleportEffect:Destroy()
                end
                if CheckArrestable(v) then
                    for a = 1, 200 do
                        if game.Players.LocalPlayer.Backpack:FindFirstChild("Handcuffs") then
                            local Handcuff = game:GetService("Players").LocalPlayer.Backpack.Handcuffs.Handle
                            Handcuff.Parent.Parent = game:GetService("Players").LocalPlayer.Character
                            game:GetService("ReplicatedStorage").Event:FireServer("Equip", game:GetService("Players").LocalPlayer.Character, Handcuff)
                        end
                        if v.Team.Name == "Prisoners" then
                            break
                        end
                        local tpbypass = workspace.Pyramid.Tele.Core2
                        tpbypass.Transparency, tpbypass.CanCollide = 1, false
                        tpbypass.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                        task.wait()
                        workspace.Pyramid.Tele.Core2.CFrame = CFrame.new(1231.141845703125, 51051.234375, 381.09619140625)
                        tpbypass.Transparency, tpbypass.CanCollide = 0, true
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
                        game:GetService("ReplicatedStorage").Event:FireServer("Eject", v)
                        game:GetService("ReplicatedStorage").Event:FireServer("Arrest", v)
                        task.wait()
                    end
                end
            end
            game.Players.LocalPlayer.Character.UpperTorso:Destroy()
        end)
