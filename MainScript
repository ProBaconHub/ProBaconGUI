local PlayersTab = Window:NewTab("Players")

local Folder = nil
if getgenv().esponplradded then
	getgenv().esponplradded:Disconnect()
end
function CreateFolder()
    if game:GetService("CoreGui"):FindFirstChild("ProBaconHubESP") then
		repeat
			game:GetService("CoreGui"):FindFirstChild("ProBaconHubESP"):Destroy()
			task.wait(.000001)
		until not game:GetService("CoreGui"):FindFirstChild("ProBaconHubESP")
    end
	task.wait()
	Folder = Instance.new("Folder", game:GetService("CoreGui"))
	Folder.Name = "ProBaconHubESP"
end
CreateFolder()
function AddNameTag(plr)
	task.spawn(function()
		if plr.Character then
			repeat
				task.wait(0.1)
			until not plr or not plr.Character or (plr.Character:FindFirstChild("HumanoidRootPart") and plr.Character:FindFirstChild("Humanoid"))
			if plr.Character then
				local Player = plr.Character
				local BGui = Instance.new("BillboardGui", Folder)
				local Frame = Instance.new("Frame", BGui)
				local TextLabel = Instance.new("TextLabel", Frame)
				while Folder and Folder.Parent and BGui and Frame and TextLabel and Player and plr.Team and task.wait(0.001) do
					TextLabel.BackgroundTransparency = 1
					Frame.BackgroundTransparency = 1
					TextLabel.BackgroundTransparency = 1
					if getgenv().ProBaconEspSettings.Tag.TeamColor then
						TextLabel.TextColor3 = plr.TeamColor.Color
					else
						TextLabel.TextColor3 = getgenv().ProBaconEspSettings.Tag.Color
					end
					TextLabel.Font = Enum.Font.SourceSansBold
					TextLabel.Size = UDim2.new(1, 0, 1, 0)
					if game.Players.LocalPlayer.Character and Player:FindFirstChild("HumanoidRootPart") and Player:FindFirstChild("Humanoid") then
						if Player:FindFirstChild("Humanoid").Health ~= 0 then
        		    		BGui.Adornee = Player:FindFirstChild("HumanoidRootPart")
	    		    		BGui.StudsOffset = Vector3.new(0, 5, 0)
	    		    		BGui.AlwaysOnTop = true
	    		    		BGui.MaxDistance = math.huge
	    		    		BGui.Size = UDim2.new(9999, 0, 9999, 0)
	    		    		Frame.Size = UDim2.new(1, 0, 1, 0)
							TextLabel.TextSize = getgenv().ProBaconEspSettings.Tag.Size
							if getgenv().ProBaconEspSettings.Tag.Name or getgenv().ProBaconEspSettings.Tag.Health or getgenv().ProBaconEspSettings.Tag.Distance then
								TextLabel.TextTransparency = 0
							else
								TextLabel.TextTransparency = 1
							end
							local tagoutput = ""
							if getgenv().ProBaconEspSettings.Tag.Name then
								tagoutput = tagoutput .. Player.Name
							end
							if getgenv().ProBaconEspSettings.Tag.Health then
								tagoutput = tagoutput .. " [" .. Player.Humanoid.Health .. "/" .. Player.Humanoid.MaxHealth .. "]"
							end
							if getgenv().ProBaconEspSettings.Tag.Distance then
								if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
							    	local distance = (Player.HumanoidRootPart.Position - game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).Magnitude
							    	tagoutput = tagoutput .. " | " .. math.round(distance)
								end
							end
							TextLabel.Text = tagoutput
						else
							break
        		        end
						TextLabel.BackgroundTransparency = 1
	    		    	Frame.BackgroundTransparency = 1
	    		    	TextLabel.BackgroundTransparency = 1
					else
						break
					end
				end
				if BGui then
					BGui:Destroy()
				end
			end
		end
	end)
end
function AddOutline(plr)
	task.spawn(function()
		if plr.Character then
			repeat
				task.wait(0.1)
			until not plr or not plr.Character or (plr.Character:FindFirstChild("HumanoidRootPart") and plr.Character:FindFirstChild("Humanoid"))
			if plr.Character then
				local Player = plr.Character
				local Highlight = Instance.new("Highlight", Folder)
				Highlight.Adornee = Player
				while Folder and Folder.Parent and Highlight and Player and plr.Team and task.wait(0.001) do
					if getgenv().ProBaconEspSettings.Outline.Outline then
						Highlight.OutlineTransparency = 0
					else
						Highlight.OutlineTransparency = 1
					end
					if getgenv().ProBaconEspSettings.Outline.TeamColor then
						Highlight.OutlineColor = plr.TeamColor.Color
					else
						Highlight.OutlineColor = getgenv().ProBaconEspSettings.Outline.OutLineColor
					end
					if getgenv().ProBaconEspSettings.Fill.TeamColor then
						Highlight.FillColor = plr.TeamColor.Color
					else
						Highlight.FillColor = getgenv().ProBaconEspSettings.Fill.FillColor
					end
					Highlight.FillTransparency = getgenv().ProBaconEspSettings.Fill.FillTransparency
				end
				if Highlight then
					Highlight:Destroy()
				end
			end
		end
	end)
end
function applyesp(plr)
	AddNameTag(plr)
	AddOutline(plr)
end
function LoadPlayerESP()
	for _, plr in pairs(game.Players:GetChildren()) do
		if plr ~= game.Players.LocalPlayer then
			applyesp(plr)
			plr.CharacterAdded:Connect(function()
				applyesp(plr)
			end)
		end
	end
end
game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
	print("Player spawned")
	CreateFolder()
	LoadPlayerESP()
end)
function RefreshESP()
	CreateFolder()
	LoadPlayerESP()
end
getgenv().esponplradded = game:GetService("Players").PlayerAdded:Connect(function(plr)
	if plr ~= game.Players.LocalPlayer then
		applyesp(plr)
	end
end)
LoadPlayerESP()

